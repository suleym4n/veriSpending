@model VeriSpending1.Models.spending

@{
    Layout = null;
}

<h2 style="text-align:center">Masraf Red Formu</h2>

@using (Html.BeginForm("IndexDecline", "Spending", FormMethod.Post))
{
    <div class="form-group">
        <strong>Red nedeni başlık:</strong>
        @Html.TextBox("mail", null, new { @class = "form-control" })
    </div>

    <div class="form-group">
        <strong>Red nedeni açıklama:</strong>
        @Html.TextBox("password", null, new { @class = "form-control" })
    </div>
    <input type="submit" class=" btn btn-primary" value="Gönder" />
}
